{
		"name": "gadgets",
		"version": "0.0.9",
		"author": "jmquigley <jmquigley@outlook.com>",
		"description": "Reusable React UI widgets - This is my widget library. There are many like it, but this one is mine...",
		"private": false,
		"license": "LGPL-3.0",
		"repository": {
				"type": "git",
				"url": "https://github.com/jmquigley/gadgets"
		},
		"bugs": {
				"url": "https://github.com/jmquigley/gadgets/issues"
		},
		"engines": {
				"node": ">=7.5"
		},
		"main": "./dist/bundle.js",
		"types": "./index.d.ts",
		"scripts": {
				"all": "yarn install && yarn run clean && yarn run lint && yarn run build && yarn run build:demo && yarn run docs",
				"build": "pkgwrap --build --webpack",
				"build:demo": "cd demo && yarn run build",
				"clean": "pkgwrap --clean",
				"coverage": "pkgwrap --coverage --jest",
				"demo": "yarn run all && yarn start",
				"docs": "pkgwrap --docs",
				"lint": "pkgwrap --lint",
				"start": "electron demo/.",
				"test": "pkgwrap --testing --jest",
				"watch": "webpack --watch --progress",
				"webpack": "webpack"
		},
		"dependencies": {
				"debug": "^3.1.0",
				"util.join": "^0.0.14",
				"util.pkgwrap": ">=0.0.70"
		},
		"devDependencies": {
				"@types/debug": "^0.0.30",
				"@types/enzyme": "^2.8.10",
				"@types/fs-extra": "^4.0.2",
				"@types/highlight.js": "^9.1.10",
				"@types/jest": "^21.1.2",
				"@types/jquery": "^3.2.12",
				"@types/lodash": "^4.14.76",
				"@types/lorem-ipsum": "^1.0.2",
				"@types/node": "^8.0.32",
				"@types/quill": "^1.3.3",
				"@types/react": "^16.0.8",
				"@types/react-dom": "^16.0.0",
				"@types/react-modal": "^2.2.1",
				"@types/sinon": "^2.3.5",
				"@types/sprintf-js": "^1.1.0",
				"awesome-typescript-loader": "^3.2.3",
				"babel-cli": "^6.26.0",
				"babel-minify-webpack-plugin": "^0.2.0",
				"babel-plugin-css-modules-transform": "^1.2.7",
				"babel-plugin-transform-runtime": "^6.23.0",
				"babel-preset-env": "^1.6.0",
				"babel-preset-minify": "^0.2.0",
				"babel-preset-power-assert": "^1.0.0",
				"babel-preset-react": "^6.24.1",
				"copy-webpack-plugin": "^4.1.0",
				"coveralls": "^3.0.0",
				"css-loader": "^0.28.7",
				"css-ripple-effect": "^1.0.5",
				"electron": "1.7.8",
				"enzyme": "^3.1.0",
				"enzyme-adapter-react-16": "^1.0.1",
				"enzyme-to-json": "^3.0.0-beta6",
				"extract-text-webpack-plugin": "^3.0.1",
				"file-loader": "^1.1.4",
				"font-awesome": "^4.7.0",
				"highlight.js": "^9.12.0",
				"identity-obj-proxy": "^3.0.0",
				"jest": "^21.2.1",
				"jquery": "^3.2.1",
				"js-output-loader": ">=0.0.5",
				"json-loader": "^0.5.7",
				"jsonfile": "^4.0.0",
				"lodash": "^4.17.4",
				"lorem-ipsum": "^1.0.4",
				"postcss-cssnext": "^3.0.2",
				"postcss-import": "^11.0.0",
				"postcss-loader": "^2.0.6",
				"postcss-modules": "^0.8.0",
				"power-assert": "^1.4.4",
				"precss": "^2.0.0",
				"process-nextick-args": "^1.0.7",
				"quill": "^1.3.2",
				"quill-markup": "^0.0.7",
				"react": "^16.0.0",
				"react-dom": "^16.0.0",
				"react-modal": "2.2.4",
				"react-select": "1.0.0-rc.5",
				"react-test-renderer": "^16.0.0",
				"sinon": "^4.0.0",
				"sprintf-js": "^1.1.1",
				"style-loader": "^0.19.0",
				"tslint": "^5.7.0",
				"tslint-react": "^3.2.0",
				"typescript": "^2.5.3",
				"typescript-babel-jest": "^1.0.5",
				"url-loader": "^0.5.9",
				"util.calc": "^0.0.3",
				"util.classnames": "^0.0.11",
				"util.ds": "^0.0.31",
				"util.keys": "^0.0.7",
				"util.matches": "^0.0.4",
				"util.promise": "^0.0.14",
				"util.toolbox": "0.0.49",
				"util.wait": "0.0.27",
				"webpack": "^3.6.0",
				"webpack-bundle-analyzer": "^2.9.0"
		},
		"pkgwrap": {
				"cleanup": [
						"docs"
				]
		},
		"jest": {
				"collectCoverage": true,
				"moduleFileExtensions": [
						"js",
						"jsx",
						"json",
						"ts",
						"tsx"
				],
				"moduleNameMapper": {
						"\\.(css|style)$": "identity-obj-proxy"
				},
				"notify": true,
				"setupFiles": [
						"<rootDir>/bin/workaround.js",
						"<rootDir>/bin/setupEnzyme.js"
				],
				"snapshotSerializers": [
						"enzyme-to-json/serializer"
				],
				"testRegex": "__test__/.*\\.test\\.(ts|tsx)$",
				"transform": {
						"^.+\\.(ts|tsx)$": "typescript-babel-jest"
				},
				"verbose": false
		}
}
