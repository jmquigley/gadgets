  "pkgwrap": {
    "bail": true,
    "cleanup": [
      "docs",
      "bin/*.d.ts",
      "bin/*.js",
      "demo/src/**/*.js",
      "demo/src/**/*.jsx",
      "demo/src/**/*.d.ts",
      "demo/main.js",
      "demo/main.d.ts",
      "demo/app.js",
      "demo/app.jsx",
      "demo/app.d.ts",
      "index.d.ts",
      "index.js",
      "lib/**/*.d.ts",
      "lib/**/*.js",
      "lib/**/*.jsx"
    ],
    "include": [
      "demo/src/**/*.{ts,tsx}"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/bin/",
      "<rootDir>/node_modules/",
      "<rootDir>/test"
    ],
    "globals": {
      "ts-jest": {
        "bableConfig": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|style)$": "identity-obj-proxy"
    },
    "notify": false,
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>/bin/workaround.js",
      "<rootDir>/bin/setupEnzyme.js",
      "<rootDir>/bin/setupDOM.js"
    ],
    "setupFilesAfterEnv": [
				"<rootDir>/bin/setupJest.js"
		],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/bin/",
      "<rootDir>/node_modules/"
    ],
    "verbose": false,
    "watch": false
  },
  "scripts": {
    "all": "yarn install && cross-env NODE_ENV=production yarn run build && yarn run docs && yarn test",
    "build": "yarn clean && yarn run lint && pkgwrap --build --webpack && yarn run build:demo",
    "build:demo": "cd demo && yarn run build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage --jest",
    "demo": "yarn run all && yarn start",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "start": "cross-env NODE_ENV=production electron demo/.",
    "test": "cross-env NODE_ENV=test jest",
    "watch": "webpack --watch --progress",
    "webpack": "webpack"
  }
}
